package constructor;

public class User {
	public static void main(String[] args) {

		Car.wheels=40;
		
		
		Car kia=new Car();
		kia.color="silver";
		kia.model="Seltos";
		kia.engine=1200;
	
		
		
		kia.features();
		
		Car vw= new Car();
		vw.color= "white";
		vw.model="Polo";
		vw.engine=1600;
		vw.features();
		
		Car honda= new Car();
		honda.color="black";
		honda.engine=1600;
		honda.model="City";
		honda.features();
		
		
		
	}
	
	
	
	
	

}
