package oops.abstraction;

//1. You can't have concrete methods inside an Concrete class
//2. An abstract class can have concrete methods

public abstract class Sprint1 extends MSWord {


	public void save() {
		
		System.out.println("Save feature");
		
	}

	
	public void insert() {
		
		System.out.println("Insert feature");
	}


		
	}

	
	
}
